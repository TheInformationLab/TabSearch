{
  "name": "TabSearch for Tableau Server™",
  "version": "3.0",
  "manifest_version": 2,
  "description": "Created by Craig Bloodworth, The Information Lab powered by Tableau",
  "background": {
    "persistent": false,
    "scripts": ["js/bin/background.js"]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
	"http://*/*","https://*/*","storage","tabs","cookies","gcm"
  ]
}
